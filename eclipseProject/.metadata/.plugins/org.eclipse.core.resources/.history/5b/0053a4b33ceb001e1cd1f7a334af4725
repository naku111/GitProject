		boolean[][] one=new boolean[a][a];
		for(int i=0;i<a;i++) {
			for(int j=0;j<a;j++) {
				one[i][j]=true;
			}
		}
		int[][] last=new int[a][a];
		for(int i=0;i<a;i++) {
			for(int j=0;j<a;j++) {
				if(one[i][j]==true)
				{
					last[i][j]=0;
				}
			}
		}
		for(int i=0;i<a;i++) {
			for(int j=0;j<a;j++) {
				
			}
		}
		
